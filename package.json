{
  "name": "hypermedia-website",
  "scripts": {
    "css": "tailwindcss -i ./assets/css/styles.css -o ./assets/css/tailwind.css --watch",
    "development": "nuxt dev",
    "dev": "concurrently \"tailwindcss -i ./assets/css/styles.css -o ./assets/css/tailwind.css --watch\" \"nuxt dev\"",
    "build": "tailwindcss -i ./assets/css/styles.css -o ./assets/css/tailwind.css && nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.13",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/supabase": "^0.3.5",
    "nuxt": "^3.4.2",
    "vuetify": "^3.3.5"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxt/types": "^2.16.3",
    "@nuxt/typescript-build": "^3.0.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/tailwindcss": "^6.6.7",
    "concurrently": "^8.0.1",
    "material-design-icons-iconfont": "^6.7.0",
    "typescript": "^4.9.5"
  }
